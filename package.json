    "web": "expo start --web",
    "web:dev": "EXPO_PUBLIC_ENV=development expo start --web",
    "web:test": "EXPO_PUBLIC_ENV=development expo start --web --port 19006",
    "build:ios:prod": "eas-cli build --platform ios --profile production",
    "build:ios:prod:submit": "eas-cli build --platform ios --profile production --auto-submit",
